{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2015",
    "lib": [
      "es5",
      "es6",
      "dom"
    ],
    "outDir": "dist",
    "rootDir": ".",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "types": [
      "core-js",
      "node"
    ],
    "typeRoots": [
      "node_modules/@types"
    ],
    "baseUrl": "./src",
    "paths": {
      "ui": [
        "ui"
      ],
      "core": [
        "core"
      ],
      "data": [
        "data"
      ],
      "assets": [
        "assets"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src"
  ],
  "awesomeTypescriptLoaderOptions": {
    "useWebpackText": true
  },
  "angularCompilerOptions": {
    "debug": false
  },
  "compileOnSave": false,
  "buildOnSave": false,
  "atom": {
    "rewriteTsconfig": false
  }
}
